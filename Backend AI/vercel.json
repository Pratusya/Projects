{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm install && npx prisma generate && npm run build",
  "outputDirectory": "dist"
}